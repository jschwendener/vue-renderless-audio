{
  "name": "vue-renderless-audio",
  "version": "0.1.1",
  "description": "Build your own audio player using a renderless Vue component",
  "keywords": ["vue", "audio", "music", "sound", "player", "renderless", "component"],
  "author": {
    "name": "Jonas Schwendener",
    "email": "jonas.schwendener@gmail.com"
  },
  "repository": "https://github.com/jschwendener/vue-renderless-audio",
  "bugs": "https://github.com/jschwendener/vue-renderless-audio/issues",
  "private": false,
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vue-cli-service build --target lib --name renderlessAudio src/RenderlessAudio.vue",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/renderlessAudio.umd.js",
  "unpkg": "dist/renderlessAudio.umd.min.js",
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
